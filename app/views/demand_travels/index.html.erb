<p id="notice"><%= notice %></p>

<h1>Listing Demand Travels</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Offer travel</th>
      <th>Accept</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @demand_travels.each do |demand_travel| %>
      <tr>
        <td><%= demand_travel.user %></td>
        <td><%= demand_travel.offer_travel %></td>
        <td><%= demand_travel.accept %></td>
        <td><%= demand_travel.state %></td>
        <td><%= link_to 'Show', demand_travel %></td>
        <td><%= link_to 'Edit', edit_demand_travel_path(demand_travel) %></td>
        <td><%= link_to 'Destroy', demand_travel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Demand travel', new_demand_travel_path %>
