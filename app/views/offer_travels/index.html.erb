<p id="notice"><%= notice %></p>

<h1>Listing Offer Travels</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Vechicle</th>
      <th>Coordenada start</th>
      <th>Coordenada end</th>
      <th>Name start</th>
      <th>Name end</th>
      <th>State</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @offer_travels.each do |offer_travel| %>
      <tr>
        <td><%= offer_travel.user %></td>
        <td><%= offer_travel.vechicle %></td>
        <td><%= offer_travel.coordenada_start %></td>
        <td><%= offer_travel.coordenada_end %></td>
        <td><%= offer_travel.name_start %></td>
        <td><%= offer_travel.name_end %></td>
        <td><%= offer_travel.state %></td>
        <td><%= link_to 'Show', offer_travel %></td>
        <td><%= link_to 'Edit', edit_offer_travel_path(offer_travel) %></td>
        <td><%= link_to 'Destroy', offer_travel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Offer travel', new_offer_travel_path %>
